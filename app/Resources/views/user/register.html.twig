{% extends 'base.html.twig' %}
{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                {{ form_start(form) }}
                {{ form_row(form.email) }}
                {{ form_row(form.plainPassword.first, {'label':'Password'}) }}
                {{ form_row(form.plainPassword.second, {'label':"Confirm Password"}) }}
                <button type="submit" class="btn-success btn" formnovalidate>Register</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>


{% endblock %}